<view class='page'>
  <view class='container'>
    <view class='header'>
      <view class='gre-logo'>
        <view class='gre'>GRE</view>
        <view>Practice Test</view>
      </view>
      <view class='buttons'>
        <view class='btn-box' wx:if='{{false}}'>
          <view class='btn btnFirst' bindtap='bindExitTap'>
            <view>Exit</view>
            <view>secton</view>
          </view>
          <view class='btn' bindtap='bindReviewTap'>
            <view>Review</view>
            <view class='iconfont icon-shuqian'></view>
          </view>
          <view class='btn' bindtap='bindMarkTap'>
            <view>Mark</view>
            <view class='iconfont icon-artboard61'></view>
          </view>
          <view class='btn' bindtap='bindBackTap'>
            <view>Back</view>
            <view class='iconfont icon-zuoyoufanzhuanjiantou'></view>
          </view>
          <view class='btn' bindtap='bindNextTap'>
            <view>Next</view>
            <view class='iconfont icon-zuoyoufanzhuanjiantou'></view>
          </view>
        </view>

        <view class='button' wx:if='{{false}}' bindtap='bindContinueTap'>
          <view>Continue</view>
          <view class='iconfont icon-youjiantou'></view>
        </view>

        <view class='threeButton' wx:if='{{true}}'>
          <view class='btn' bindtap='bindReviewTap'>
            <view>Review</view>
            <view class='iconfont icon-shuqian'></view>
          </view>
          <view class='btn' bindtap='bindReturnTap'>
            <view>Return</view>
            <view class='iconfont icon-zuoyoufanzhuanjiantou'></view>
          </view>
          <view class='btn continue-btn' bindtap='bindContinueTap'>
            <view>Continue</view>
            <view class='iconfont icon-youjiantou'></view>
          </view>
        </view>
      </view>
    </view>
    <view class='section'>
      <view class='sectionContent' wx:if='{{true}}'>
        <view>Section 1 of 2</view>
        <view wx:if='{{true}}'>00:13:56</view>
      </view>
    </view>
    <view class='content'>
      <view class='content-item1' wx:if='{{false}}'>
        <view class='content-title' wx:if='{{false}}'>
          <view class='title-h2'>General Test Information</view>
        </view>
        <view class='subheadBox'>
