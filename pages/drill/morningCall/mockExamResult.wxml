          <view class='star'>
            <view class='iconfont icon-difficulty_fills'></view>
            <view class='iconfont icon-difficulty_fills'></view>
            <view class='iconfont icon-difficulty_fills'></view>
            <view class='iconfont icon-difficulty_fills'></view>
            <view class='iconfont icon-difficulty_fills'></view>
          </view>
            <view class='answerTime'>
              <view class='iconfont icon-spenttime'></view>
              <view>00:30</view>
            </view>
            <view class='iconfont icon-right1'></view>
          </view>
        </view>
        <view class='content-right' bindtap='bindExplainTap'>
          <view class='iconfont icon-play'></view>
          <view class='vio-explain'>视频讲解</view>
        </view>
      </view>
